<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Conteúdo03"
             x:Class="AppGallery.XamarinForms.Paginas.PaginaDeNavegacao.Conteudo03">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Inserir Página na Pilha" Clicked="InserirPaginaPilha" />
            <Button Text="Remover Página na Pilha" Clicked="RemoverPaginaPilha" />
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand"
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Voltar" Clicked="VoltarParaPaginaAnterior" />
            <Button Text="Voltar para o início" Clicked="VoltarParaInicio" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>